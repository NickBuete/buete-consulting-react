{
  "name": "server",
  "version": "1.0.0",
  "description": "API service for Buete Consulting dashboard",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run prisma:generate && tsc --project tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "dotenv -- prisma generate",
    "prisma:deploy": "dotenv -- prisma migrate deploy",
    "prisma:push": "dotenv -- prisma db push",
    "prisma:studio": "dotenv -- prisma studio",
    "prisma:migrate:dev": "dotenv -- prisma migrate dev",
    "seed:admin": "ts-node scripts/createAdmin.ts",
    "db:local": "docker-compose up postgres -d",
    "db:local:stop": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.4",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0",
    "@types/nodemailer": "^7.0.4",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.12",
    "openai": "^6.15.0",
    "pg": "^8.13.1",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "twilio": "^5.11.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.16.0",
    "prisma": "7.2.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
